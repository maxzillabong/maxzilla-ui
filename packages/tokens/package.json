{
  "name": "maxzilla-ui-tokens",
  "version": "0.1.2",
  "description": "Design tokens and theme system for Maxzilla UI",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./css": {
      "import": "./dist/tokens.css"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "design-tokens",
    "css-variables",
    "theme",
    "maxzilla-ui"
  ],
  "author": "Max Vananen <m.f.vananen@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  }
}
